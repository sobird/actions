{
  "name": "@sobird/actions",
  "version": "0.0.1",
  "description": "Impl act runner using typescript",
  "type": "module",
  "main": "index.js",
  "bin": {
    "actions": "./cmd/index.ts"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:runner": "rollup -cw --environment NODE_ENV:development",
    "build:buf": "buf generate https://gitea.com/gitea/actions-proto-def.git",
    "build:runner": "rollup -c --environment NODE_ENV:production",
    "eslint:fix": "eslint src --fix",
    "test": "vitest",
    "export": "export NODE_OPTIONS=--import=./node_modules/@sobird/ts-node/register.js",
    "node": "NODE_OPTIONS=--import=./node_modules/@sobird/ts-node/register.js node",
    "cmd": "NODE_OPTIONS=--import=./node_modules/@sobird/ts-node/register.js node cmd",
    "lint:eslint": "eslint src --ext ts,js --report-unused-disable-directives --max-warnings 10",
    "preversion": "yarn lint:eslint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags",
    "postinstall": "echo \"postinstall\"",
    "prepare": "husky"
  },
  "keywords": [
    "act",
    "act-runner",
    "actions",
    "github-actions"
  ],
  "author": "sobird",
  "license": "MIT",
  "dependencies": {
    "@actions/glob": "^0.4.0",
    "@bufbuild/protobuf": "^1.10.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@commander-js/extra-typings": "^12.0.1",
    "@connectrpc/connect": "^1.6.1",
    "@connectrpc/connect-next": "^1.6.1",
    "@connectrpc/connect-node": "^1.6.1",
    "@types/better-sqlite3": "^7.6.11",
    "async-mutex": "^0.5.0",
    "better-sqlite3": "^11.5.0",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "git-url-parse": "^14.0.0",
    "ignore": "^5.3.1",
    "ip": "^2.0.1",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "next": "^15.0.3",
    "prompts": "^2.4.2",
    "rc": "^1.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "retry": "^0.13.1",
    "shell-quote": "^1.8.1",
    "simple-git": "^3.24.0",
    "tar": "^7.1.0",
    "xdg-basedir": "^5.1.0",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.47.2",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@connectrpc/protoc-gen-connect-es": "^1.6.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sobird/ts-node": "^1.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/dockerode": "^3.3.28",
    "@types/express": "^4.17.21",
    "@types/git-url-parse": "^9.0.3",
    "@types/hosted-git-info": "^3.0.5",
    "@types/ip": "^1.1.3",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.17.6",
    "@types/prompts": "^2.4.9",
    "@types/rc": "^1.2.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/retry": "^0.12.5",
    "@types/shell-quote": "^1.7.5",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.20.2",
    "eslint-config-sobird": "^0.2.14",
    "express": "^4.19.2",
    "glob": "^10.3.10",
    "husky": "^9.0.11",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.49",
    "release-please": "^16.10.2",
    "rollup": "^4.14.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "resolutions": {
    "strip-ansi": "6.0.0"
  }
}
